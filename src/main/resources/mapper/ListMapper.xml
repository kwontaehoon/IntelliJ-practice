<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.springBoot.board.mapper.ListMapper">

    <select id="getUserId" resultType="Member">
        select *
        from Member m
        where m.id = #{id}
    </select>

</mapper>